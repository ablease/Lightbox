
<form action="/login" method='post' id='loginForm'>

  <div class="row">
    <div class="large-4 columns">
      <label>Email
        <input type='email' name='email' placeholder="Email">
      </label>
    </div>
  </div>

  <div class="row">
    <div class="large-4 columns">
      <label>Password
        <input type='password' name='password' placeholder='Password'>
      </label>
    </div>
  </div>

  <div class="row loginGParent">
    <div id="loginParent">
      <input type="submit" value="Log In" class="button" id='loginBtn'>
    </div>
  </div>

</form>
